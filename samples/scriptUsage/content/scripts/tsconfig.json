{
  "$schema": "https://raw.githubusercontent.com/TypeScriptToLua/TypeScriptToLua/master/tsconfig-schema.json",
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    "moduleResolution": "Node",
    "types": [],
    "strict": true,
    "outDir": "out.lua",
    "experimentalDecorators": true
  },
  "tstl": {
    "luaTarget": "5.4",
    "noImplicitSelf": true
  }
}